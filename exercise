
            var numberToGuess = Math.floor(Math.random() * 10) + 1;
            var guess = 0;

            do {
                guess = parseInt(prompt("Guess the number (1 to 10):"));
                if (guess === numberToGuess) {
                    alert("Correct! You guessed the number!");
                } else {
                    alert("Wrong guess, try again!");
                }
            } while (guess !== numberToGuess); 
        

       
        function guessNumberWhile() {
            var numberToGuess = Math.floor(Math.random() * 10) + 1; 
            var guess = 0; 

            while (guess !== numberToGuess) {
                guess = parseInt(prompt("Guess the number (1 to 10):"));
                if (guess === numberToGuess) {
                    alert("Correct! You guessed the number!");
                } else {
                    alert("Wrong guess, try again!");
                }
            }
        }